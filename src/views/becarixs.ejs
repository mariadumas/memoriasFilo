<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./partials/head.ejs") %>
</head>

<body>
    <%- include("./partials/header.ejs") %>

        <main>

            <h2 class="title-detail becarixs">Becarixs</h2>

            <div class="accordion accordion-flush" id="accordionFlushExample">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="flush-headingOne">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                            data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                            UBA
                            <span class="badge rounded-pill">
                                <%= uba %>
                            </span>
                        </button>
                    </h2>
                    <div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne"
                        data-bs-parent="#accordionFlushExample">
                        <div class="accordion-body">
                            <table class="interior-acordion becas">
                                <% tesistas.forEach(tesista=> { %>
                                    <% if ([1, 2, 3, 4, 5, 11].includes(tesista.proyectoTesisBeca.id)) { %>
                                        <td>


                                            <% if (tesista.tipo_nivel==="Grado" ) { %>

                                                <p class="nivel-becas">Grado</p>

                                                <p class="tesista">
                                                        <%= tesista.proyectoTesisIntegrante.nombre %>
                                                            <%= tesista.proyectoTesisIntegrante.apellido %>
                                                    
                                                <p>
                                                    <% if (tesista.titulo_proyecto) { %>
                                                        <p>"<%= tesista.titulo_proyecto%>"</p>
                                                        <% } %>

                                                            <p>
                                                                <% if (tesista.proyectoTesisDirector.gender===1) { %>
                                                                    <span><em>Directora:</em></span>
                                                                    <% } else { %>
                                                                        <span><em>Director:</em></span>
                                                                        <% } %>
                                                                            <em>
                                                                                <%=
                                                                                    tesista.proyectoTesisDirector.nombre%>
                                                                                    <%= tesista.proyectoTesisDirector.apellido
                                                                                        %>
                                                                            </em>
                                                            </p>
                                                            <% if (tesista.proyectoTesisCodirector) { %>
                                                                <p>Codirector/a: <%=
                                                                        tesista.proyectoTesisCodirector.nombre%>
                                                                        <%= tesista.proyectoTesisCodirector.apellido %>
                                                                </p>
                                                                <% } %>
                                                                    <% } %>

                                                                        <% if (tesista.tipo_nivel==="Doctorado" ) { %>
                                                                            <p class="nivel-becas">Doctorado<span
                                                                                    class="badge rounded-pill">
                                                                                    <%= doctoradoUBA %>
                                                                                </span></p>

                                                                            <p class="tesista">
                                                                                    <%= tesista.proyectoTesisIntegrante.nombre
                                                                                        %>
                                                                                        <%= tesista.proyectoTesisIntegrante.apellido
                                                                                            %>
                                                                                
                                                                            <p>
                                                                                <% if (tesista.titulo_proyecto) { %>
                                                                                    <p>"<%= tesista.titulo_proyecto%>"
                                                                                    </p>
                                                                                    <% } %>
                                                                                        <p>
                                                                                            <% if
                                                                                                (tesista.proyectoTesisDirector.gender===1)
                                                                                                { %>
                                                                                                <span><em>Directora:</em></span>
                                                                                                <% } else { %>
                                                                                                    <span><em>Director:</em></span>
                                                                                                    <% } %>
                                                                                                        <em>
                                                                                                            <%=
                                                                                                                tesista.proyectoTesisDirector.nombre%>
                                                                                                                <%= tesista.proyectoTesisDirector.apellido
                                                                                                                    %>
                                                                                                        </em>
                                                                                        </p>
                                                                                        <% if
                                                                                            (tesista.proyectoTesisCodirector)
                                                                                            { %>
                                                                                            <p>Codirector/a: <%=
                                                                                                    tesista.proyectoTesisCodirector.nombre%>
                                                                                                    <%= tesista.proyectoTesisCodirector.apellido
                                                                                                        %>
                                                                                            </p>
                                                                                            <% } %>
                                                                                                <% } %>

                                                                                                    <% if
                                                                                                        (tesista.tipo_nivel==="Posdoctorado"
                                                                                                        ) { %>
                                                                                                        <p
                                                                                                            class="nivel-becas">
                                                                                                            Posdoctorado
                                                                                                        </p>

                                                                                                        <p class="tesista">
                                                                                                                <%= tesista.proyectoTesisIntegrante.nombre
                                                                                                                    %>
                                                                                                                    <%= tesista.proyectoTesisIntegrante.apellido
                                                                                                                        %>
                                                                                                            
                                                                                                        <p>
                                                                                                            <% if
                                                                                                                (tesista.titulo_proyecto)
                                                                                                                { %>
                                                                                                                <p>"<%=tesista.titulo_proyecto%>"</p>
                                                                                                                <% } %>
                                                                                                                    <p>
                                                                                                                        <% if
                                                                                                                            (tesista.proyectoTesisDirector.gender===1)
                                                                                                                            {
                                                                                                                            %>
                                                                                                                            <span><em>Directora:</em></span>
                                                                                                                            <% } else
                                                                                                                                {
                                                                                                                                %>
                                                                                                                                <span><em>Director:</em></span>
                                                                                                                                <% }
                                                                                                                                    %>
                                                                                                                                    <em>
                                                                                                                                        <%=
                                                                                                                                            tesista.proyectoTesisDirector.nombre%>
                                                                                                                                            <%= tesista.proyectoTesisDirector.apellido
                                                                                                                                                %>
                                                                                                                                    </em>
                                                                                                                    </p>
                                                                                                                    <% if
                                                                                                                        (tesista.proyectoTesisCodirector)
                                                                                                                        {
                                                                                                                        %>
                                                                                                                        <p>Codirector/a:
                                                                                                                            <%=
                                                                                                                                tesista.proyectoTesisCodirector.nombre%>
                                                                                                                                <%= tesista.proyectoTesisCodirector.apellido
                                                                                                                                    %>
                                                                                                                        </p>
                                                                                                                        <% }
                                                                                                                            %>
                                                                                                                            <% }
                                                                                                                                %>

                                        </td>

                                        </tr>
                                        <% } %>
                                            <% }) %>
                            </table>

                        </div>
                    </div>
                </div>
